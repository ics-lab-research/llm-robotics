def move_joint_relative():
    local Waypoint_1_from_p=p[-0.13, -0.26, 0.14, -0.001, 3.11, 0.03]
    local Waypoint_1_to_p=p[-0.04, -0.2, 0.11, -0.004, 3.11, -0.18]
    while (True):
        movej(pose_add(get_target_tcp_pose(), pose_sub(Waypoint_1_to_p, Waypoint_1_from_p)), a=1.39, v=1.04)
    end
end
