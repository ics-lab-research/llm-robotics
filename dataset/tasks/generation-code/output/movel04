def move_linear():
    local Waypoint_1_p=p[-0.13, -0.26, 0.14, -0.001, 3.11, 0.03] 
    local Waypoint_2_p=p[-0.008, -0.26, 0.28, -0.27, -2.71, 0.217] 

    while (True):
        movel(Waypoint_1_p, a=1.2, v=0.25, r=0.05)
    	movel(Waypoint_2_p, a=0.78, v=0.87, r=0.06)
    end
end
